    <header>
        @*<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light box-shadow mb-3">
                <div class="container">
                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">SailWithDivine</a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                        <ul class="navbar-nav flex-grow-1">
                            <li class="nav-item">
                                <a class="nav-link main-color" asp-area="" asp-controller="Home" asp-action="Boat">Boat</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link main-color" asp-area="" asp-controller="Home" asp-action="Position">Position</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>*@
    </header>
    <main role="main">
        @RenderBody()
    </main>

    @*<footer class="footer text-muted">
            <div class="container">
                &copy; 2020 - sailwithdivine.com
            </div>
        </footer>*@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
